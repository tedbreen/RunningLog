<header>
  <h1>Runs Index View</h1>
</header>

  <% runs.each( function(run) { %>

  <div class="row container" id="index-run-feed">

    <div class="col-xs-2">
      <div><%= run.escape( 'start_date' ) %></div>
      <div><%= justTime( run.escape('start_time') ) %></div>
    </div>

    <div class="col-xs-3">
      <div>
        <a href="#/users/<%= run.escape( 'user_id' ) %>">
    <%= runs.users().getOrFetch( run.getUserId() ).get('fname') + " " +
        runs.users().getOrFetch( run.getUserId() ).get('lname') %>
        </a>
      </div>
    </div>

    <div class="col-xs-7">
      <div>
        <a href="#/runs/<%= run.escape( 'id' ) %>">
          <%= run.escape( 'title' ) %>
        </a>
      </div>
      <div><%= run.escape( 'distance' ) + " mi"%></div>
    </div>

  </div>

  <%  }) %>
